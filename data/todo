//check if either database or table is empty

$dbname = isset($_GET['db']) ? $_GET['db'] : "";
$tablename = isset($_GET['table']) ? $_GET['table'] : "";


// Check if the database name is provided
if (empty($dbname) ||  empty($tablename)) {
    include 'error_messages/no_db_or_table.html';
    die();
}
